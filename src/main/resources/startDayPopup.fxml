<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="85.0" prefWidth="200">
        <children>
            <VBox alignment="CENTER" layoutX="-1.0" prefHeight="130.0" prefWidth="242.0">
                <children>
               <VBox alignment="CENTER" prefHeight="180.0" prefWidth="242.0">
                  <children>
                     <Label prefHeight="29.0" prefWidth="171.0" text="Choose a start day for the week" />
                  </children>
               </VBox>
                    <ChoiceBox fx:id="startDayChoiceBox" prefHeight="21.0" prefWidth="202.0">
                        <VBox.margin>
                            <Insets left="20.0" right="20.0" top="15.0" />
                        </VBox.margin>
                        <padding>
                            <Insets left="20.0" right="20.0" />
                        </padding>
                    </ChoiceBox>
               <VBox alignment="CENTER" prefHeight="200.0" prefWidth="242.0">
                  <children>
                     <Button fx:id="chooseDayBtn" mnemonicParsing="false" prefHeight="35.0" prefWidth="93.0" style="-fx-background-color: #cde3c5;" text="Choose Day" />
                  </children>
               </VBox>
                </children>
            </VBox>
        </children></AnchorPane>
</Scene>
